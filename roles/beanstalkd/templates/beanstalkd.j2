## Defaults for the beanstalkd init script, /etc/init.d/beanstalkd on
## Debian systems.

BEANSTALKD_LISTEN_ADDR={{ beanstalkd.listen_address }}
BEANSTALKD_LISTEN_PORT={{ beanstalkd.listen_port }}

# You can use BEANSTALKD_EXTRA to pass additional options. See beanstalkd(1)
# for a list of the available options. Uncomment the following line for
# persistent job storage.
BEANSTALKD_EXTRA="-b {{ beanstalkd.persistent_job_storage }} -z {{ beanstalkd.maximum_job_size }}"
